        };

            //<>
        td_lessgreaterus: td_lessgreaterus {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&hm GT LT>, <&lessgreaterus>;
        };

            // German
            // <>
        td_lessgreaterde: td_lessgreaterde {
