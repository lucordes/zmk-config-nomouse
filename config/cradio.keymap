        ;
    };
            // ue
    ueusint: ueusint {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings 
        = <&macro_tap &kp LS(APOS) &kp U>
        ;
    };
            // ae
    aeusint: aeusint {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
