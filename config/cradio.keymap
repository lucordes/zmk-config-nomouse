//ZMK_COMBO(pwr,  &kp C_POWER, 0 4, ALL)
    combos {
        compatible = "zmk,combos";
        combo_pwr {
            timeout-ms = <50>;
            key-positions = <0 4>;
            bindings = <&kp C_POWER>;
        };

            combo_five {
            timeout-ms = <50>;  // You may adjust this value based on your requirements
            key-positions = <3 13>;
            bindings = <&kp N5>;
        };
            combo_sixde {
            timeout-ms = <50>;  // You may adjust this value based on your requirements
            key-positions = <6 16>;
            bindings = <&kp N6>;
            layers = <BASEDE SYMDE>;
        };
            combo_sixus {
            timeout-ms = <50>;  // You may adjust this value based on your requirements
            key-positions = <6 16>;
            bindings = <&number6us>;
            layers = <BASEUSINT SYMUSINT>;
        };
            combo_up {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <11 12 13>;
            bindings = <&kp UP>;
        };
            combo_down {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <11 13>;
            bindings = <&kp DOWN>;
        };
            combo_right {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <12 13>;
            bindings = <&kp RIGHT>;
        };
            combo_left {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <11 12>;
            bindings = <&kp LEFT>;
        };

        combo_uprhand {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <16 17 18>;
            bindings = <&kp UP>;
        };
            combo_downrhand {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <16 18>;
            bindings = <&kp DOWN>;
        };
            combo_rightrhand {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <17 18>;
            bindings = <&kp RIGHT>;
        };
            combo_leftrhand {
            timeout-ms = <20>;  // You may adjust this value based on your requirements
            key-positions = <16 17>;
            bindings = <&kp LEFT>;
        };

            combo_scCombo {
            timeout-ms = <50>;  // You may adjust this value based on your requirements
            key-positions = <12 22>;
            bindings = <&scModMorph>;
            layers = <BASEDE BASEUSINT>;
        };
            combo_ecommaCombo {
            timeout-ms = <50>;  // You may adjust this value based on your requirements
            key-positions = <17 27>;
            bindings = <&ecomma>;
            layers = <BASEDE BASEUSINT>;
        };
    };







//macros

macros {
    ecomma: ecomma {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp E &kp COMMA>
        ;
    };
    sc: sc {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings
            = <&macro_tap &kp S &kp C>
            ;
    };
    capitalSlowC: capitalSlowC {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp LS(S) &kp C>
        ;
    };
    bracketde: bracketde {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp DE_LEFT_BRACKET &kp DE_RIGHT_BRACKET &kp LEFT>
        ;
    };
    bracede: bracede {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp DE_LEFT_BRACE &kp DE_RIGHT_BRACE &kp LEFT>
        ;
    };
    parende: parende {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp DE_LEFT_PARENTHESIS &kp DE_RIGHT_PARENTHESIS &kp LEFT>
        ;
    };
    lessgreaterde: lessgreaterde {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp DE_LESS_THAN &kp DE_GREATER_THAN &kp LEFT>
        ;
    };
    bracketus: bracketus {
