        ;
    };
    // "
    qouteusint: qouteusint {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp LS(APOS) &kp SPC>
        ;
    };
    // '
    singleqouteusint: singleqouteusint {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings
        = <&macro_tap &kp APOS &kp SPC>
        ;
    };
        // ö
    oeusint: oeusint {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
