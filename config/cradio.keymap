//mod morph
 
    behaviors {
        //home row mods

        hm: hm {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            global-quick-tap;
            require-prior-idle-ms = <125>;
            bindings = <&kp>, <&kp>;
        };
        hmhold: hmhold {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            bindings = <&mo>, <&kp>;
        };
        //mod morph
            // SL(.)->:
        germanStyleDot: germanStyleDot {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp DOT>, <&kp COLON>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            // SL(,)->;
        germanStyeComma: germanStyeComma {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp COMMA>, <&kp SEMICOLON>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };

            // German numbers
            //3'
        number3de: number3de {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F3 N3>, <&kp DE_SQT>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            //4$
        number4de: number4de {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F4 N4>, <&hm DE_EURO DE_DOLLAR>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            //8#
        number8de: number8de {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F8 N8>, <&kp DE_HASH>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            //9?
        number9de: number9de {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F9 N9>, <&kp DE_QMARK>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };

            // US Numbers
            //2"
        number2us: number2us {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F2 N2>, <&dqtspace>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            //2"
        number2usbase: number2usbase{
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F2 N2>, <&kp DQT>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            //3'
        number3us: number3us {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F3 N3>, <&sqtspace>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
            //3'
        number3usbase: number3usbase{
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&hm F3 N3>, <&kp SQT>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
